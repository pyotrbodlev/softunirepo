<main>
  <h1>Browser</h1>
  <form action="/" method="get">
    <input type="text" name="s" class="search" placeholder="Search...">
    <input type="number" name="f" class="difficulty" placeholder="Difficulty from...">
    <span>-</span>
    <input type="number" name="t" class="difficulty" placeholder="Difficulty to...">
    <input type="submit" value="search">
  </form>
  {{#each cubes}}
    <div class="cube">
      <img class="cube"
        src="{{imageUrl}}">
      <p class="name">{{name}}</p>
      <p><span>Difficulty level:</span> {{difficultyLevel}}</p>
      <a class="btn" href="/details/{{_id}}">Details</a>
      <a class="btn" href="/attach/accessory/{{_id}}">Attach Accessory</a>
    </div>
  {{/each}}
</main>